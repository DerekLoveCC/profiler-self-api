<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>JetBrains.Profiler.SelfApi</RootNamespace>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <!--
	<SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>Subplatform.Snk</AssemblyOriginatorKeyFile>
	-->
    <Version>1.0.0</Version>
    <Company>JetBrains s.r.o.</Company>
    <Copyright>Copyright © 2019 JetBrains s.r.o.</Copyright>
    <Authors>Mikhail Pilin, Ilya Ivanov</Authors>
    <Description>???</Description>
    <PackageReleaseNotes>• Initial release provides all basic functionality.</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/JetBrains/profiler-self-api</RepositoryUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <FileVersion>1.0.0.0</FileVersion>
    <PackageIconUrl>https://resources.jetbrains.com/storage/products/resharper/img/meta/ReSharper_64.png</PackageIconUrl>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageVersion>0.3.1</PackageVersion>
    <TargetFrameworks>net46;netcoreapp3.0</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="_ReSharper.Caches\**" />
    <EmbeddedResource Remove="_ReSharper.Caches\**" />
    <None Remove="_ReSharper.Caches\**" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.IO.Compression" Condition="'$(TargetFramework)' == 'net45'" />
    <Reference Include="System.IO.Compression" Condition="'$(TargetFramework)' == 'net46'" />
    <Reference Include="System.Net.Http" Condition="'$(TargetFramework)' == 'net46'" />
    <Reference Include="System.Net.Http, Version=4.2.1.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" Condition="'$(TargetFramework)' == 'net45'">
      <HintPath>..\..\Program Files\dotnet\packs\Microsoft.NETCore.App.Ref\3.0.0-preview6-27804-01\ref\netcoreapp3.0\System.Net.Http.dll</HintPath>
    </Reference>
    <Reference Include="System.Runtime" Condition="'$(TargetFramework)' == 'net45'" />
  </ItemGroup>
  <PropertyGroup>
    <FrameworkPathOverride Condition="'$(TargetFramework)' == 'net20' or '$(TargetFramework)' == 'net30' or '$(TargetFramework)' == 'net35'">$(MSBuildProgramFiles32)\Reference Assemblies\Microsoft\Framework\.NETFramework\v3.5\Profile\Client</FrameworkPathOverride>
  </PropertyGroup>
</Project>
